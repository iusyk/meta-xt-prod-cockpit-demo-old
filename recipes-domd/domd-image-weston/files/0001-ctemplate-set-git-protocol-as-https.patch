From 87a14affc8f89c53daa8bac53ff43952cf082678 Mon Sep 17 00:00:00 2001
From: Ihor Usyk <ihor_usyk@epam.com>
Date: Fri, 8 Apr 2022 11:08:25 +0000
Subject: [PATCH] ctemplate: set git protocol as 'https'

git protocol is deprecated.

Signed-off-by: Ihor Usyk <ihor_usyk@epam.com>
---
 meta-rcar-gen3-adas/recipes-support/ctemplate/ctemplate_git.bb | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/meta-rcar-gen3-adas/recipes-support/ctemplate/ctemplate_git.bb b/meta-rcar-gen3-adas/recipes-support/ctemplate/ctemplate_git.bb
index 02b2bc37..549712e9 100644
--- a/meta-rcar-gen3-adas/recipes-support/ctemplate/ctemplate_git.bb
+++ b/meta-rcar-gen3-adas/recipes-support/ctemplate/ctemplate_git.bb
@@ -1,6 +1,6 @@
 require ctemplate.inc
 
-SRC_URI = "git://github.com/svn2github/ctemplate.git"
+SRC_URI = "git://github.com/svn2github/ctemplate.git;protocol=https"
 SRC_URI[md5sum] = ""
 SRC_URI[sha256sum] = ""
 SRCREV = "${AUTOREV}"
-- 
